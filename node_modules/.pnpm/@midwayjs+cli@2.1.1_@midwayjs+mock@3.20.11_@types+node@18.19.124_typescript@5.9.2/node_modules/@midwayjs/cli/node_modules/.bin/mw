#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/@midwayjs+cli@2.1.1_@midwayjs+mock@3.20.11_@types+node@18.19.124_typescript@5.9.2/node_modules/@midwayjs/cli/bin/node_modules:/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/@midwayjs+cli@2.1.1_@midwayjs+mock@3.20.11_@types+node@18.19.124_typescript@5.9.2/node_modules/@midwayjs/cli/node_modules:/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/@midwayjs+cli@2.1.1_@midwayjs+mock@3.20.11_@types+node@18.19.124_typescript@5.9.2/node_modules/@midwayjs/node_modules:/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/@midwayjs+cli@2.1.1_@midwayjs+mock@3.20.11_@types+node@18.19.124_typescript@5.9.2/node_modules:/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/@midwayjs+cli@2.1.1_@midwayjs+mock@3.20.11_@types+node@18.19.124_typescript@5.9.2/node_modules/@midwayjs/cli/bin/node_modules:/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/@midwayjs+cli@2.1.1_@midwayjs+mock@3.20.11_@types+node@18.19.124_typescript@5.9.2/node_modules/@midwayjs/cli/node_modules:/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/@midwayjs+cli@2.1.1_@midwayjs+mock@3.20.11_@types+node@18.19.124_typescript@5.9.2/node_modules/@midwayjs/node_modules:/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/@midwayjs+cli@2.1.1_@midwayjs+mock@3.20.11_@types+node@18.19.124_typescript@5.9.2/node_modules:/Users/shenhongshi/Documents/GitHub/gateway/midway-demo-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/midway-bin.js" "$@"
else
  exec node  "$basedir/../../bin/midway-bin.js" "$@"
fi
