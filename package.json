{
  "name": "midway-ts-server",
  "version": "1.0.0",
  "description": "基于MidwayJS框架的TypeScript服务器，用于对gateway-go-server、gateway-ts-sdk、openapi-ts-sdk、openapi-ts-sdk-cli进行系统性测试",
  "type": "commonjs",
  "main": "dist/bootstrap.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "start": "node ./dist/bootstrap.js",
    "start:dev": "./scripts/start.sh",
    "stop": "./scripts/stop.sh",
    "clean": "midway-bin clean",
    "test:all": "npm run test:go-server && npm run test:ts-sdk && npm run test:client && npm run test:generator && npm run test:integration",
    "test:go-server": "npm run test -- --grep \"Gateway Go Server\"",
    "test:ts-sdk": "npm run test -- --grep \"Gateway TS SDK\"",
    "test:client": "npm run test -- --grep \"TS SDK Client\"",
    "test:generator": "npm run test -- --grep \"TS SDK Generator\"",
    "test:integration": "npm run test -- --grep \"Integration Tests\"",
    "test:start-services": "node scripts/start-services.js",
    "test:stop-services": "node scripts/stop-services.js"
  },
  "dependencies": {
    "@midwayjs/bootstrap": "^3.0.0",
    "@midwayjs/core": "^3.0.0",
    "@midwayjs/decorator": "^3.0.0",
    "@midwayjs/info": "^3.0.0",
    "@midwayjs/koa": "^3.0.0",
    "@midwayjs/logger": "^3.0.0",
    "@midwayjs/swagger": "^3.20.13",
    "@midwayjs/validate": "^3.0.0",
    "@midwayjs/web": "^3.0.0",
    "axios": "^1.6.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.1.1",
    "koa": "^2.13.1",
    "openapi-ts-sdk": "https://github.com/langgexyz/openapi-ts-sdk.git#semver:^1.0.0",
    "tslib": "^2.8.1",
    "ts-concurrency": "https://github.com/langgexyz/ts-concurrency#semver:^0.2.0",
    "ts-streamclient": "https://github.com/langgexyz/ts-streamclient#semver:^2.0.0",
    "ts-xutils": "https://github.com/langgexyz/ts-xutils#semver:^0.4.0",
    "ts-json": "https://github.com/langgexyz/ts-json#semver:^0.2.0",
    "ws": "^8.14.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@midwayjs/cli": "^2.0.0",
    "@midwayjs/mock": "^3.0.0",
    "@types/jest": "^29.2.0",
    "@types/koa": "^2.13.4",
    "@types/node": "^18.0.0",
    "@types/ws": "^8.5.5",
    "cross-env": "^7.0.3",
    "jest": "^29.2.2",
    "mwts": "^1.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "./"
  },
  "author": "Test Team",
  "license": "MIT"
}